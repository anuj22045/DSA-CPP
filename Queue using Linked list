#include<iostream>
using namespace std;
struct node{
int data;
node *next;
};
node *temp, *nn, *start=nullptr;

void enqueue(int n){
nn=new node;
nn->data=n;
nn->next=nullptr;
if(start==nullptr){
    start=nn;
}
else{
    temp=start;
    while(temp->next!=nullptr){
        temp=temp->next;
    }
    temp->next=nn;
}
}
void dequeue(){
if(start==nullptr){
    cout<<"queue is empty"<<endl;
}
else{
    temp=start;
    start=start->next;
    delete temp;
}
}

void display(){
temp=start;
while(temp!=nullptr){
    cout<<temp->data<<"\t";
    temp=temp->next;
}
}
int main(){
enqueue(10);
enqueue(20);
enqueue(30);
cout<<"enqueued the element int the queue"<<endl;
display();
cout<<endl;
dequeue();
cout<<"after dequeue"<<endl;
display();
return 0;
}
